{
  "$schema": "https://gofastmcp.com/public/schemas/fastmcp.json/v1.json",
  "source": {
    "path": "mcp_server_snowflake/server.py",
    "entrypoint": "server"
  },
  "environment": {
    "python": ">=3.11",
    "dependencies": [
      "fastmcp>=2.8.1,<3.0.0",
      "mcp[cli]>=1.9.4,<2.0.0",
      "pydantic>=2.11.4,<3.0.0",
      "pyyaml>=6.0.2,<7.0.0",
      "requests>=2.32.3,<3.0.0",
      "snowflake-connector-python>=3.15.0,<4.0.0",
      "snowflake-core>=1.0.0,<2.0.0",
      "sqlglot>=27.8.0,<30.0.0"
    ]
  },
  "deployment": {
    "transport": "http",
    "host": "0.0.0.0",
    "port": 8000,
    "path": "/mcp",
    "log_level": "INFO"
  }
}
